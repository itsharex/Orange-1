<?xml version="1.0" encoding="utf-8"?>
<MsixPackagingToolTemplate
    xmlns="http://schemas.microsoft.com/msix/packaging/msixpackagingtool/template/2022">
    <Settings
        AllowTelemetry="false"
        ApplyACLsToPackageFiles="true"
        GenerateCommandLineFile="true"
        AllowPromptForPassword="false">
    </Settings>
    <Installer
        Path="orange"
        Arguments=""
        InstallLocation="C:\Program Files\FruitsAI\Orange">
    </Installer>
    <PackageInformation
        PackageName="Orange"
        PackageDisplayName="Orange"
        PublisherName="CN=FruitsAI"
        PublisherDisplayName="FruitsAI"
        Version="0.1.0.0"
        PackageDescription="FruitsAI Orange Desktop App">
        <Capabilities>
            <Capability Name="runFullTrust" />
            
        </Capabilities>
        <Applications>
            <Application
                Id="com.willxue.fruitsai.orange"
                Description="FruitsAI Orange Desktop App"
                DisplayName="Orange"
                ExecutableName="orange"
                EntryPoint="Windows.FullTrustApplication">
                
            </Application>
        </Applications>
        <Resources>
            <Resource Language="en-us" />
        </Resources>
        <Dependencies>
            <TargetDeviceFamily Name="Windows.Desktop" MinVersion="10.0.17763.0" MaxVersionTested="10.0.19041.0" />
        </Dependencies>
        <Properties>
            <Framework>false</Framework>
            <DisplayName>Orange</DisplayName>
            <PublisherDisplayName>FruitsAI</PublisherDisplayName>
            <Description>FruitsAI Orange Desktop App</Description>
            <Logo>Assets\AppIcon.png</Logo>
        </Properties>
    </PackageInformation>
    <SaveLocation PackagePath="orange.msix" />
    <PackageIntegrity>
        <CertificatePath></CertificatePath>
    </PackageIntegrity>
</MsixPackagingToolTemplate>
